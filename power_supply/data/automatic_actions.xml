<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="check_contract_dates" model="base.automation">
        <field name="name">=> Avoid overlaped supply contract dates</field>
        <field name="model_id" ref="power.contract"/>
        <field name="state">code</field>
        <field name="code">
            record._check_valid_date()
        </field>
        <field name="trigger">on_create_or_write</field>
        <!--<field name="on_change_fields">date_start,date_end,supply_id</field>-->
        <field name="active" eval="True"/>
    </record>

</odoo>
