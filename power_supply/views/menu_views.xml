<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- WINDOW ACTIONS: -->
    <record id="action_power_supply" model="ir.actions.act_window">
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">action</field>
        <field name="domain" eval="False"/>
        <field name="filter" eval="False"/>
        <field name="limit">80</field>
        <field name="name">Power supplies</field>
        <field name="res_model">power.supply</field>
        <field name="search_view_id" eval="False"/>
        <field name="target">current</field>
        <field name="type">ir.actions.act_window</field>
        <field name="usage" eval="False"/>
        <field name="view_id" eval="False"/>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_power_pricelist" model="ir.actions.act_window">
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">action</field>
        <field name="domain" eval="False"/>
        <field name="filter" eval="False"/>
        <field name="limit">80</field>
        <field name="name">Power pricelist</field>
        <field name="res_model">power.pricelist</field>
        <field name="search_view_id" eval="False"/>
        <field name="target">current</field>
        <field name="type">ir.actions.act_window</field>
        <field name="usage" eval="False"/>
        <field name="view_id" eval="False"/>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_power_contract_type" model="ir.actions.act_window">
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">action</field>
        <field name="domain" eval="False"/>
        <field name="filter" eval="False"/>
        <field name="limit">80</field>
        <field name="name">Contract type</field>
        <field name="res_model">power.contract.type</field>
        <field name="search_view_id" eval="False"/>
        <field name="target">current</field>
        <field name="type">ir.actions.act_window</field>
        <field name="usage" eval="False"/>
        <field name="view_id" eval="False"/>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_power_contract" model="ir.actions.act_window">
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">action</field>
        <field name="domain" eval="False"/>
        <field name="filter" eval="False"/>
        <field name="limit">80</field>
        <field name="name">Contracts</field>
        <field name="res_model">power.contract</field>
        <field name="search_view_id" eval="False"/>
        <field name="target">current</field>
        <field name="type">ir.actions.act_window</field>
        <field name="usage" eval="False"/>
        <field name="view_id" eval="False"/>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_power_tag" model="ir.actions.act_window">
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">action</field>
        <field name="domain" eval="False"/>
        <field name="filter" eval="False"/>
        <field name="limit">80</field>
        <field name="name">Tags</field>
        <field name="res_model">power.tag</field>
        <field name="search_view_id" eval="False"/>
        <field name="target">current</field>
        <field name="type">ir.actions.act_window</field>
        <field name="usage" eval="False"/>
        <field name="view_id" eval="False"/>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_power_contact" model="ir.actions.act_window">
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">action</field>
        <field name="domain" eval="False"/>
        <field name="context">{'search_default_dealer_marketeer': 1}</field>
        <field name="filter" eval="False"/>
        <field name="limit">80</field>
        <field name="name">Contacts</field>
        <field name="res_model">res.partner</field>
        <field name="search_view_id" eval="False"/>
        <field name="target">current</field>
        <field name="type">ir.actions.act_window</field>
        <field name="usage" eval="False"/>
        <field name="view_id" eval="False"/>
        <field name="view_mode">kanban,tree,form</field>
    </record>

    <!-- MENUS: -->
    <menuitem
            action="action_power_supply"
            id="power_supply_main_menu"
            name="Power Supply"
            sequence="1"
            web_icon="power_supply,static/description/credentials.png"
    />

    <record id="power_supply_menu" model="ir.ui.menu">
        <field name="action" ref="action_power_supply"/>
        <field name="active" eval="True"/>
        <field name="name">Power Supply</field>
        <field name="parent_id" ref="power_supply_main_menu"/>
        <field name="sequence">10</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="power_contract_menu" model="ir.ui.menu">
        <field name="action" ref="action_power_contract"/>
        <field name="active" eval="True"/>
        <field name="name">Contracts</field>
        <field name="parent_id" ref="power_supply_main_menu"/>
        <field name="sequence">20</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="power_contact_menu" model="ir.ui.menu">
        <field name="action" ref="action_power_contact"/>
        <field name="active" eval="True"/>
        <field name="name">Contacts</field>
        <field name="parent_id" ref="power_supply_main_menu"/>
        <field name="sequence">30</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="power_configuration_menu" model="ir.ui.menu">
        <field name="active" eval="True"/>
        <field name="name">Configuration</field>
        <field name="parent_id" ref="power_supply_main_menu"/>
        <field name="sequence">40</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="power_pricelist_menu" model="ir.ui.menu">
        <field name="action" ref="action_power_pricelist"/>
        <field name="active" eval="True"/>
        <field name="name">Pricelist</field>
        <field name="parent_id" ref="power_configuration_menu"/>
        <field name="sequence">10</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="power_contract_type_menu" model="ir.ui.menu">
        <field name="action" ref="action_power_contract_type"/>
        <field name="active" eval="True"/>
        <field name="name">Contract type</field>
        <field name="parent_id" ref="power_configuration_menu"/>
        <field name="sequence">20</field>
        <field name="web_icon" eval="False"/>
    </record>

    <record id="power_tag_menu" model="ir.ui.menu">
        <field name="action" ref="action_power_tag"/>
        <field name="active" eval="True"/>
        <field name="name">Tags</field>
        <field name="parent_id" ref="power_configuration_menu"/>
        <field name="sequence">30</field>
        <field name="web_icon" eval="False"/>
    </record>


</odoo>